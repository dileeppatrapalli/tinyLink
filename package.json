{
  "name": "tinylink",
  "version": "1.0.0",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "migrate": "psql \"$DATABASE_URL\" -f migrations/001_create_links.sql",
    "migrate:node": "node scripts/migrate.js",
    "release": "npm run migrate:node",
    "seed": "psql \"$DATABASE_URL\" -f seeds/seed_links.sql",
    "test": "jest --runInBand"
  },
  "dependencies": {
    "dotenv": "^17.2.3",
    "ejs": "^3.1.10",
    "ejs-mate": "^4.0.0",
    "express": "^4.18.2",
    "express-ejs-layouts": "^2.5.1",
    "express-rate-limit": "^6.7.0",
    "helmet": "^7.0.0",
    "pg": "^8.11.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
   
    "jest": "^29.6.0",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3"
   
  }
}
